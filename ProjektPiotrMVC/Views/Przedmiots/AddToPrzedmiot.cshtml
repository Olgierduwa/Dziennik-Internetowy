@model ProjektPiotrMVC.Models.ViewsModels.AddToPrzedmiotViewModel
@{
    ViewBag.Title = "AddToPrzedmiot";
}

<h2>Dodaj klasę do przedmiotu</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.PrzedmiotID)
    <div class="form-group">
        <div class="col-md-12">
            <br />
        </div>
        <div class="col-md-12">
            @Html.DropDownList("KlasaID", null, htmlAttributes: new { @class = "form-control" })
        </div>
        <div class="col-md-12">
            <br />
        </div>
        <div class="col-md-12">
            <input type="button" style="width:100px" value="Anuluj" class="btn btn-default" onclick="location.href = '@Url.Action("Edit", "Przedmiots", new { id = Model.PrzedmiotID })'" />
            <input type="submit" style="width:100px" value="Dodaj" class="btn btn-success" />
        </div>
        <div class="col-md-12">
            <br />
        </div>
    </div>
}

